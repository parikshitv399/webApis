@{
    var title = "First Index Page";
    var key = "Id";
    var value = "Value";
    List<string> strs = new List<string>() { "Eena", "Meena", "Deeka" };
}
<h1>@title</h1>
<hr />
<img src="~/images/R.png"/>
<table style="border-width:2px;
              border-style: solid;">
    <tr>
        <td>@key</td>
        <td>@value</td>
    </tr>
    @{
        var counter = 1;
        foreach (var item in strs)
        {
                <tr>
                    <td>@counter</td>
                    <td>@item</td>
                </tr>
            counter++;
        }
    }
</table>